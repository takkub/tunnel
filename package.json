{
  "name": "tunnel",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "check": "node scripts/check-requirements.js",
    "check-dns": "node scripts/check-dns.js",
    "check-cnames": "node -e \"require('dotenv').config();require('./scripts/cloudflare-api').listDnsRecords(process.env.ZONE_ID,process.env.CLOUDFLARE_API_TOKEN).then(r=>r.filter(x=>x.type==='CNAME'&&x.content.includes('cfargotunnel.com')).forEach(x=>console.log(x.name)))\"",
    "login": "node scripts/login.js",
    "setup": "node scripts/setup-tunnel.js",
    "cleanup": "node scripts/cleanup-tunnel.js",
    "status": "node scripts/status.js",
    "tunnel:app:up": "docker-compose -f docker-compose-cloudflare-app.yml up -d",
    "tunnel:app:down": "docker-compose -f docker-compose-cloudflare-app.yml down",
    "tunnel:app:logs": "docker-compose -f docker-compose-cloudflare-app.yml logs -f",
    "tunnel:app:restart": "npm run tunnel:app:down && npm run tunnel:app:up",
    "start": "npm run tunnel:app:up",
    "stop": "npm run tunnel:app:down"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "typescript": "^5.5.3"
  },
  "private": true
}
